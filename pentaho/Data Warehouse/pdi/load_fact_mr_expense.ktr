  <step>
    <name>mr_expenses</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DeltasPharma</connection>
    <sql>SELECT dt.line_id, dt.product_id, dt.salary_amount, dt.promotional_amount, dt.sample_amount, dt.ppp, &#xd;&#xa;&#x28;dt.non_salary_expense_amount - &#x28;dt.promotional_amount&#x2b;dt.ppp&#x2b;dt.sample_amount&#x29;&#x29;as other_expenses, date_value, &#xd;&#xa;dt.product_name, dt.expense_id, dt.user_id, dt.ppp_reference as ppp_reference, dt.write_date as last_update&#xd;&#xa;FROM&#x28;SELECT expl.id as line_id,&#xd;&#xa;expl.product_id,&#xd;&#xa;CASE WHEN expl.product_id &#x3d; 29&#xd;&#xa;	THEN expl.unit_amount &#x2a; expl.unit_quantity&#xd;&#xa;	ELSE 0.0&#xd;&#xa;END as salary_amount,&#xd;&#xa;CASE WHEN expl.product_id &#x21;&#x3d; 29&#xd;&#xa;	THEN expl.unit_amount &#x2a; expl.unit_quantity&#xd;&#xa;	ELSE 0.0&#xd;&#xa;END as non_salary_expense_amount,&#xd;&#xa;CASE WHEN prd.categ_id &#x3d; 17&#xd;&#xa;	THEN expl.unit_amount &#x2a; expl.unit_quantity&#xd;&#xa;	ELSE 0.0&#xd;&#xa;END as promotional_amount,&#xd;&#xa;CASE WHEN prd.categ_id &#x3d; 22&#xd;&#xa;	THEN expl.unit_amount &#x2a; expl.unit_quantity&#xd;&#xa;	ELSE 0.0&#xd;&#xa;END as sample_amount,&#xd;&#xa;CASE WHEN prd.categ_id &#x3d; 39&#xd;&#xa;	THEN expl.unit_amount &#x2a; expl.unit_quantity&#xd;&#xa;	ELSE 0.0&#xd;&#xa;END as ppp,&#xd;&#xa;expl.date_value,&#xd;&#xa;expl.name as product_name,&#xd;&#xa;exp.id as expense_id,&#xd;&#xa;usr.id as user_id,&#xd;&#xa;expl.write_date,&#xd;&#xa;expl.ppp_reference&#xd;&#xa;FROM hr_expense_line expl &#xd;&#xa;	LEFT JOIN hr_expense_expense exp ON exp.id &#x3d; expl.expense_id&#xd;&#xa;	JOIN hr_employee emp ON emp.id &#x3d; exp.employee_id&#xd;&#xa;	JOIN resource_resource res ON res.id &#x3d; emp.resource_id&#xd;&#xa;	LEFT JOIN res_users usr ON usr.id &#x3d; res.user_id&#xd;&#xa;	JOIN product_template prd ON prd.id &#x3d; expl.product_id&#xd;&#xa;WHERE exp.state in &#x28;&#x27;draft&#x27;, &#x27;accepted&#x27;&#x29; &#xd;&#xa;		AND expl.write_date &#x3e; &#x3f;&#x29; dt</sql>
    <limit>0</limit>
    <lookup>mr_expenses_last_update</lookup>
    <execute_each_row>Y</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
